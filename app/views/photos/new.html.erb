<div class="container-groupe-margin">
  <h1>Ajouter une nvlle photo !!!</h1>

  <%= form_for ([@groupe, @photo]), :html => {:multipart => true} do |f| %>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, size: "30x5" %>
  </p>
  <p>
    <%= f.file_field :photo, multiple: true %>
  </p>

  <p><%= f.submit %></p>
<% end %>

</div>_
