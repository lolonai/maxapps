<div class="container-groupe-margin">
  <div class="groupe_list">
    <div class="list_box">
        <h3 class="list_title">Liste des membres</h3>
        <br>
        <hr>

      <div class="list_content">
        <p> <%= @groupe.admin.name %> - Administateur</p>
        <% @membres.each do |membre| %>
          <% if membre.user.id == @groupe.admin.id %>
          <% else %>
          <p> <%= membre.user.name%> </p>

          <% end %>


        <% end %>
      </div>
    </div>
  </div>
</div>

